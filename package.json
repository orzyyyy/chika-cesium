{
  "name": "chika-cesium",
  "version": "0.0.0",
  "description": "A modal loader using cesium",
  "scripts": {
    "start": "nino go -c ./scripts/nino.go.js -e ./src/demo",
    "test": "nino test",
    "build": "tsc && nino koei -c ./scripts/nino.koei.js",
    "compile": "tsc && nino compile",
    "codecov": "nino test -d",
    "dev": "nino koei -w-d",
    "lint": "eslint .",
    "prettier": "nino prettier",
    "lint-staged": "lint-staged",
    "backup": "npm config set registry=https://registry.npmjs.org && npm publish && npm config set registry=https://registry.npm.taobao.org",
    "pub": "npm run build && npm run compile && git push origin master && npm publish",
    "deploy": "npm run build && gh-pages -d dist"
  },
  "main": "lib",
  "keywords": [
    "cesium"
  ],
  "files": [
    "lib",
    "dist"
  ],
  "author": "zy9",
  "license": "MIT",
  "devDependencies": {
    "@babel/polyfill": "^7.4.3",
    "@types/cesium": "^1.54.2",
    "@types/classnames": "^2.2.7",
    "@typescript-eslint/parser": "^1.6.0",
    "classnames": "^2.2.6",
    "dekko": "^0.2.1",
    "eslint": "^5.16.0",
    "gh-pages": "^2.0.1",
    "html-webpack-include-assets-plugin": "^1.0.7",
    "html2canvas": "^1.0.0-alpha.12",
    "lint-staged": "^8.1.5",
    "pre-commit": "^1.2.2",
    "prettier": "^1.16.4",
    "ts-loader": "^5.3.3",
    "weatherstar-switch": "^1.0.7"
  },
  "dependencies": {
    "cesium": "^1.56.1",
    "nino-cli": "^0.8.0"
  },
  "pre-commit": [
    "lint-staged"
  ],
  "lint-staged": {
    "*.{ts}": [
      "npm run prettier",
      "npm run lint",
      "git add"
    ]
  }
}
