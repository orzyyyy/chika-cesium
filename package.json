{
  "name": "chika-cesium",
  "version": "0.0.0",
  "description": "A modal loader using cesium",
  "scripts": {
    "start": "nino go -c ./scripts/nino.go.js -e ./src/demo",
    "test": "nino test",
    "dist": "nino koei -c ./scripts/nino.koei.js",
    "compile": "nino compile",
    "dev": "nino koei -w-d",
    "lint": "eslint ./src/*.ts",
    "prettier": "nino prettier",
    "lint-staged": "lint-staged",
    "backup": "npm config set registry=https://registry.npmjs.org && npm publish && npm config set registry=https://registry.npm.taobao.org",
    "pub": "npm run build && npm run compile && git push origin master && npm publish"
  },
  "keywords": [
    "cesium"
  ],
  "files": [
    "lib",
    "dist"
  ],
  "author": "zy9",
  "license": "MIT",
  "devDependencies": {
    "@types/cesium": "^1.47.3",
    "@types/classnames": "^2.2.7",
    "babel-eslint": "^10.0.1",
    "classnames": "^2.2.6",
    "dekko": "^0.2.1",
    "eslint": "^5.14.1",
    "html-webpack-include-assets-plugin": "^1.0.7",
    "lint-staged": "^8.1.5",
    "pre-commit": "^1.2.2",
    "prettier": "^1.15.3",
    "ts-loader": "^5.3.3"
  },
  "dependencies": {
    "cesium": "^1.54.0",
    "nino-cli": "^0.6.1"
  },
  "pre-commit": [
    "lint-staged"
  ],
  "lint-staged": {
    "*.{ts}": [
      "npm run prettier",
      "npm run lint",
      "git add"
    ]
  }
}
